@model Lok.Models.ShreniTaha
@{
    ViewData["Title"] = "ShreniTaha";
    ViewData["HeaderMain"] = "ShreniTaha";
    ViewData["HeaderLink"] = "ShreniTaha";
    Layout = "_layoutAdmin";

}
<partial name="_HeaderPartial" />

<form asp-action="Create" class="row form-material">

    <div class="col-md-4">

        <div class="form-group">
            <label asp-for="Name" class="control-label"></label>
            <input asp-for="Name" class="form-control" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Type" class="control-label"></label>
            @Html.DropDownList("Type", new SelectList(ViewBag.Type, "Value", "Text"), new { @class = "form-control" })
            <span asp-validation-for="Type" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
        </div>
</form>
<div>
